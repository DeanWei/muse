<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mogujie.jarvis.dao.IDMapper" >
  
  <select id="selectMaxJobId" resultType="long">
  	SELECT max(`jobId`) FROM job
  </select>
  
  <select id="selectMaxTaskId" resultType="long">
  	SELECT max(`taskId`) FROM task
  </select>
</mapper>